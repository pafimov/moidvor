<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.skilldvor">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".NewsCheck"></activity>
        <activity android:name=".NewsList" />
        <activity android:name=".AddNews" />
        <activity android:name=".RootLayoutCheck" />
        <activity android:name=".OrderList" />
        <activity android:name=".UserMenu" />
        <activity android:name=".AddUser" />
        <activity android:name=".RootMenu" />
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>